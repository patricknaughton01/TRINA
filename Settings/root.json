{
    "robot": "@robot_bubonic.json",
    "user": {
        "configs":"@anthrax/configs.json"
    },
    "MotionServer": {
        "mode": "Kinematic",
        "ip_address":"127.0.0.1",
        "port":8080,
        "components":["left_limb","right_limb"]
    },
    "RedisServer": {
        "ip_address":"localhost"
    },
